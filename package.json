{
  "name": "mashling-support",
  "description": "mashling config edit support",
  "version": "0.0.1",
  "publisher": "tbhatia",
  "engines": {
    "vscode": "^1.8.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json",
    "onLanguage:yaml",
    "onCommand:mashling.createGateway",
    "onCommand:mashling.installMashling",
    "onCommand:mashling.updateMashling"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "mashling.createGateway",
        "title": "Create Mashling Gateway",
        "category": "Mashling"
      },
      {
        "command": "mashling.installMashling",
        "title": "Install Mashling",
        "category": "Mashling"
      },
      {
        "command": "mashling.updateMashling",
        "title": "Update Mashling",
        "category": "Mashling"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Example configuration"
    },
    "jsonValidation": [
      {
        "fileMatch": "mashling.json",
        "url": "./MashlingJsonSchemas/mashlingJsonSchema.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "update-vscode": "node ./node_modules/vscode/bin/install",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.33",
    "@types/node": "^6.0.52",
    "typescript": "^2.1.5",
    "vscode": "^1.0.3"
  }
}