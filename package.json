{
  "name": "mashling-support",
  "description": "mashling config edit support",
  "version": "0.0.1",
  "publisher": "tbhatia",
  "repository": {
    "url": "https://github.com/TIBCOSoftware/mashling-vscode-extension"
  },
  "license": "",
  "engines": {
    "vscode": "^1.8.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json",
    "onCommand:mashling.createSampleGateway",
    "onCommand:mashling.createGatewayUsingGatewayDescriptor",
    "onCommand:mashling.installMashling",
    "onCommand:mashling.updateMashling"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "mashling.createSampleGateway",
        "title": "Create Sample Mashling",
        "category": "Mashling"
      },
      {
        "command": "mashling.createGatewayUsingGatewayDescriptor",
        "title": "Create Mashling using current descriptor",
        "category": "Mashling"
      },
      {
        "command": "mashling.installMashling",
        "title": "Install Mashling",
        "category": "Mashling"
      },
      {
        "command": "mashling.updateMashling",
        "title": "Update Mashling",
        "category": "Mashling"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Example configuration"
    },
    "jsonValidation": [
      {
        "fileMatch": "*.mashling.json",
        "url": "./MashlingJsonSchemas/mashlingJsonSchema.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "update-vscode": "node ./node_modules/vscode/bin/install",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.3",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}